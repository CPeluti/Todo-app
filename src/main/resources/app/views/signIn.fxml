<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="loginAnchor" prefHeight="600.0" prefWidth="800.0" styleClass="bg" stylesheets="@../styles/login.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.controllers.SignController">
    <Label layoutX="321.0" layoutY="173.0" text="Email" />
    <TextField fx:id="email" layoutX="319.0" layoutY="191.0" promptText="digite seu e-mail" />
    <Label layoutX="321.0" layoutY="227.0" text="Nome" />
    <TextField fx:id="name" layoutX="319.0" layoutY="245.0" promptText="digite seu nome" />
    <Label layoutX="321.0" layoutY="282.0" text="Senha" />
    <PasswordField fx:id="passwd" layoutX="319.0" layoutY="300.0" promptText="Digite sua senha" />
    <Label layoutX="321.0" layoutY="338.0" text="Confirme sua senha" />
    <PasswordField fx:id="passwdConfirm" layoutX="319.0" layoutY="356.0" promptText="Confirme sua senha" />
    <Button fx:id="signInButton" layoutX="350.0" layoutY="401.0" mnemonicParsing="false" onAction="#createUser" text="Cadastrar" />
</AnchorPane>
